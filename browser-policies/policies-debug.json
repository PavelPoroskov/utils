      "dom.security.https_only_mode": {
        "Value": true,
        "Status": "default"
      },
      "dom.security.https_only_mode_ever_enabled": {
        "Value": true,
        "Status": "default"
      }
    },
    "Homepage": {
      "StartPage": "previous-session"
    },
    "FirefoxHome": {
      "SponsoredTopSites": false,
      "SponsoredPocket": false
    }
